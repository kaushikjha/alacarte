<% title '<img src="/images/title_register.png" alt="" />' %>
 
<% form_for @user, :html => { :class => 'form_style' }, :url => account_path do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :invitation_token %>
  <%= render :partial => "form", :object => f %>
    <p class="submit_button_style">
  <%= image_submit_tag("/images/button_submit_long.jpg") %>
<% end %>